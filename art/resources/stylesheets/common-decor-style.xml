/* rules for navigation menu */
/* ========================================== */
ul#navmenu,
ul.sub1,
ul.sub2 {
    margin:0px;
    padding:0px;
    list-style-type:none;
    font-size:9pt;
}
ul.sub1,
ul.sub2 {
    border:1px solid #CCC;
    background-color:#FFF;
    box-shadow:3px 3px 10px 3px #CCC;
}
ul#navmenu li {
    width:100px;
    text-align:center;
    position:relative;
    float:left;
    margin-right:0px;
    font-size:10pt;
    font-weight:normal;
    color:#7a6e62;
}
ul#navmenu a {
    padding-top:1px;
    padding-bottom:3px;
    color:black;
    width:inherit;
    text-decoration:none;
    display:block;
    height:15px;
    background-image:url('img/menu_background.png'), url('img/menu_background.png');
    border:1px solid #CCC;
}
ul#navmenu .sub1 li {
    text-align:left;
    padding-right:5px;
    width:300px;
}
ul#navmenu .sub2 li {
    text-align:left;
    padding-right:5px;
    width:150px;
}
ul#navmenu .sub1 a {
    padding-top:3px;
    padding-left:5px;
    margin-top:2px;
    width:inherit;
    background-image:none;
    border:none;
}
ul#navmenu .sub2 a {
    margin-left:0px;
}
ul#navmenu li:hover &gt; a {
    background-image:url('img/menu_selected_background.png'), url('img/menu_selected_background.png');
}
ul#navmenu li:hover a:hover {
    background-image:url('img/menu_selected_background.png'), url('img/menu_selected_background.png');
}
