   <Class code="I" kind="concept">
      <Meta name="internalId" value="23001"/>
      <Rubric id="d1e138638" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Implementers receiving this property must not raise an error if the data is received, but will not perform any useful function with the data. This conformance level is not used in profiles or other artifacts that are specific to the "sender" or "initiator" of a communication.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e138654" kind="short">
