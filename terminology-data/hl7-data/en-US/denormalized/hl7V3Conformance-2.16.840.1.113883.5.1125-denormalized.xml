      </Rubric>
   </Class>
   <Class code="NP" kind="concept">
      <Meta name="internalId" value="23000"/>
      <Rubric id="d1e138666" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>All implementers are prohibited from transmitting this content, and may raise an error if they receive it.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e138682" kind="short">
         <Label xml:lang="en-US">not permitted</Label>
      </Rubric>
