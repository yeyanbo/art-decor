      <Meta name="internalId" value="22999"/>
      <Rubric id="d1e138693" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>All implementers must support this property. I.e. they must be able to transmit, or to receive and usefully handle the concept.</p>
         </Label>
      </Rubric>
