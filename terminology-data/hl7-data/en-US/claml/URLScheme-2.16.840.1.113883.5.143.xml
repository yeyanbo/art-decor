   <Class code="cid" kind="concept">
      <Meta name="internalId" value="22603"/>
      <Rubric id="d1e255568" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>cid: is the most common mechanism used in webservices. A cid: reference points from the SOAP body to other content in the mime package in which the SOAP envelope is wrapped</p>
         </Label>
      </Rubric>
