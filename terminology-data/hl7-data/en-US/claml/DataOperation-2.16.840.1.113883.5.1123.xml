      <SubClass code="SUSPEND"/>
      <Rubric id="d1e95389" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Change the status of an object representing an Act.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e95410" kind="short">
         <Label xml:lang="en-US">modify status</Label>
      </Rubric>
      <Rubric id="d1e95410" kind="preferred">
         <Label xml:lang="en-US">modify status</Label>
      </Rubric>
   </Class>
