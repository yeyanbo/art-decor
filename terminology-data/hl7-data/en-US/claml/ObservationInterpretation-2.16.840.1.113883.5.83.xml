   <Class code="_GeneticObservationInterpretation" kind="abstract">
      <Meta name="isSelectable" value="false"/>
      <Meta name="internalId" value="22646"/>
      <SubClass code="Carrier"/>
      <Rubric id="d1e162956" kind="definition">
         <Label xml:lang="en-US">
            <p>Codes that specify interpretation of genetic analysis, such as "positive", "negative", "carrier", "responsive", etc.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e162969" kind="short">
         <Label xml:lang="en-US">GeneticObservationInterpretation</Label>
      </Rubric>
      <Rubric id="d1e162969" kind="preferred">
         <Label xml:lang="en-US">GeneticObservationInterpretation</Label>
      </Rubric>
   </Class>
   <Class code="_ObservationInterpretationChange" kind="abstract">
