   <Class code="HX" kind="concept">
      <Meta name="internalId" value="21636"/>
      <SuperClass code="EX"/>
      <Rubric id="d1e163707" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Definition:</b>The numeric observation/test result is interpreted as being above the high threshold value for a particular protocol within which the result is being reported.</p>
            <p>
               <b>Example:</b>An ALT (SGOT) result above a protocol-defined threshold value of 2.5 times the upper limit of normal based on the subject's sex and age.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e163734" kind="short">
         <Label xml:lang="en-US">above high threshold</Label>
      </Rubric>
      <Rubric id="d1e163734" kind="preferred">
         <Label xml:lang="en-US">above high threshold</Label>
      </Rubric>
   </Class>
   <Class code="LX" kind="concept">
      <Meta name="internalId" value="21635"/>
      <SuperClass code="EX"/>
      <Rubric id="d1e163745" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Definition:</b>The numeric observation/test result is interpreted as being below the low threshold value for a particular protocol within which the result is being reported.</p>
            <p>
               <b>Example:</b>A Total White Blood Cell Count falling below a protocol-defined threshold value of 3000/mm^3</p>
         </Label>
      </Rubric>
      <Rubric id="d1e163772" kind="short">
         <Label xml:lang="en-US">below low threshold</Label>
      </Rubric>
      <Rubric id="d1e163772" kind="preferred">
         <Label xml:lang="en-US">below low threshold</Label>
      </Rubric>
   </Class>
