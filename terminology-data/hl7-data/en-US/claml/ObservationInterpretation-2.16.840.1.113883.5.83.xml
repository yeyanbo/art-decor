      <SubClass code="U"/>
      <SubClass code="W"/>
      <Rubric id="d1e162981" kind="definition">
         <Label xml:lang="en-US">
            <p>Change of quantity and/or severity. At most one of B or W and one of U or D allowed.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e162994" kind="short">
         <Label xml:lang="en-US">ObservationInterpretationChange</Label>
      </Rubric>
      <Rubric id="d1e162994" kind="preferred">
         <Label xml:lang="en-US">ObservationInterpretationChange</Label>
      </Rubric>
   </Class>
