      <SuperClass code="EC"/>
      <Rubric id="d1e234249" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Substitution occurred or is permitted between equivalent Generics but not Brands</p>
            <p>
               <i>Examples:</i>
            </p>
            <ul>
               <li>Lisnopril (Lupin Corp) for Lisnopril (Wockhardt Corp)</li>
            </ul>
         </Label>
      </Rubric>
      <Rubric id="d1e234282" kind="short">
         <Label xml:lang="en-US">generic composition</Label>
      </Rubric>
      <Rubric id="d1e234282" kind="preferred">
         <Label xml:lang="en-US">generic composition</Label>
      </Rubric>
   </Class>
