      <Rubric id="d1e233968" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Substitution occurred or is permitted with another product that may potentially have different ingredients, but having the same biological and therapeutic effects.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e233984" kind="short">
         <Label xml:lang="en-US">ActSubstanceAdminSubstitutionCode</Label>
      </Rubric>
      <Rubric id="d1e233984" kind="preferred">
         <Label xml:lang="en-US">ActSubstanceAdminSubstitutionCode</Label>
      </Rubric>
   </Class>
   <Class code="E" kind="concept">
      <Meta name="internalId" value="23107"/>
      <SuperClass code="_ActSubstanceAdminSubstitutionCode"/>
      <SubClass code="EC"/>
      <SubClass code="TE"/>
      <Rubric id="d1e233996" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Substitution occurred or is permitted with another bioequivalent and therapeutically equivalent product.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e234017" kind="short">
         <Label xml:lang="en-US">equivalent</Label>
      </Rubric>
      <Rubric id="d1e234017" kind="preferred">
         <Label xml:lang="en-US">equivalent</Label>
      </Rubric>
   </Class>
