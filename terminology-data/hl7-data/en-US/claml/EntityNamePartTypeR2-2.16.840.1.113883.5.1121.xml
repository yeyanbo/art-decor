   <Class code="DEL" kind="concept">
      <Meta name="internalId" value="22786"/>
      <Rubric id="d1e116307" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>A delimiter has no meaning other than being literally printed in this name representation. A delimiter has no implicit leading and trailing white space.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e116323" kind="short">
         <Label xml:lang="en-US">delimiter</Label>
      </Rubric>
      <Rubric id="d1e116323" kind="preferred">
