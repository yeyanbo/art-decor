   <Class code="D" kind="concept">
      <Meta name="internalId" value="21433"/>
      <Rubric id="d1e201900" kind="definition">
         <Label xml:lang="en-US">
            <p>The receiver may respond in a non-immediate manner. Note: this will be the default value.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e201913" kind="short">
         <Label xml:lang="en-US">deferred</Label>
      </Rubric>
      <Rubric id="d1e201913" kind="preferred">
         <Label xml:lang="en-US">deferred</Label>
      </Rubric>
   </Class>
   <Class code="I" kind="concept">
      <Meta name="internalId" value="21432"/>
      <Rubric id="d1e201925" kind="definition">
         <Label xml:lang="en-US">
            <p>The receiver is required to assume that the sender is blocking and behave appropriately by sending an immediate response.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e201938" kind="short">
         <Label xml:lang="en-US">immediate</Label>
      </Rubric>
      <Rubric id="d1e201938" kind="preferred">
         <Label xml:lang="en-US">immediate</Label>
      </Rubric>
   </Class>
   <Class code="Q" kind="concept">
      <Meta name="internalId" value="21434"/>
      <Rubric id="d1e201949" kind="definition">
         <Label xml:lang="en-US">
            <p>The receiver shall keep any application responses in a queue until such time as the queue is polled.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e201962" kind="short">
         <Label xml:lang="en-US">queue</Label>
      </Rubric>
      <Rubric id="d1e201962" kind="preferred">
         <Label xml:lang="en-US">queue</Label>
      </Rubric>
   </Class>
