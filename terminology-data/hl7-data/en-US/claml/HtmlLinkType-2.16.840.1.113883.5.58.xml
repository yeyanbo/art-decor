   <Class code="next" kind="concept">
      <Meta name="internalId" value="11021"/>
      <Rubric id="d1e139699" kind="definition">
         <Label xml:lang="en-US">
            <p>Refers to the next document in a linear sequence of documents. User agents may choose to preload the "next" document, to reduce the perceived load time.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139712" kind="short">
         <Label xml:lang="en-US">next</Label>
      </Rubric>
      <Rubric id="d1e139712" kind="preferred">
