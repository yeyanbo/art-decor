   <Class code="start" kind="concept">
      <Meta name="internalId" value="11020"/>
      <Rubric id="d1e139771" kind="definition">
         <Label xml:lang="en-US">
            <p>Refers to the first document in a collection of documents. This link type tells search engines which document is considered by the author to be the starting point of the collection.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139784" kind="short">
         <Label xml:lang="en-US">start</Label>
