      <Meta name="internalId" value="11027"/>
      <Rubric id="d1e139554" kind="definition">
         <Label xml:lang="en-US">
            <p>Refers to a document serving as a chapter in a collection of documents.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139567" kind="short">
         <Label xml:lang="en-US">chapter</Label>
