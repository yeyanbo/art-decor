   <Class code="DF" kind="concept">
      <Meta name="internalId" value="10621"/>
      <Meta name="howApplies" value="mandatory"/>
      <Rubric id="d1e91266" kind="definition">
         <Label xml:lang="en-US">
            <p>The deflate compressed data format as specified in RFC 1951 [http://www.ietf.org/rfc/rfc1951.txt].</p>
         </Label>
      </Rubric>
      <Rubric id="d1e91281" kind="short">
         <Label xml:lang="en-US">deflate</Label>
      </Rubric>
      <Rubric id="d1e91281" kind="preferred">
         <Label xml:lang="en-US">deflate</Label>
      </Rubric>
   </Class>
   <Class code="GZ" kind="concept">
      <Meta name="internalId" value="10622"/>
      <Meta name="howApplies" value="other"/>
      <Rubric id="d1e91292" kind="definition">
         <Label xml:lang="en-US">
            <p>A compressed data format that is compatible with the widely used GZIP utility as specified in RFC 1952 [http://www.ietf.org/rfc/rfc1952.txt] (uses the deflate algorithm).</p>
         </Label>
      </Rubric>
      <Rubric id="d1e91307" kind="short">
         <Label xml:lang="en-US">gzip</Label>
      </Rubric>
      <Rubric id="d1e91307" kind="preferred">
         <Label xml:lang="en-US">gzip</Label>
      </Rubric>
   </Class>
   <Class code="Z" kind="concept">
      <Meta name="internalId" value="10624"/>
      <Meta name="howApplies" value="deprecated"/>
      <Rubric id="d1e91318" kind="definition">
         <Label xml:lang="en-US">
            <p>Original UNIX compress algorithm and file format using the LZC algorithm (a variant of LZW). Patent encumbered and less efficient than deflate.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e91333" kind="short">
         <Label xml:lang="en-US">compress</Label>
      </Rubric>
      <Rubric id="d1e91333" kind="preferred">
         <Label xml:lang="en-US">compress</Label>
      </Rubric>
   </Class>
   <Class code="ZL" kind="concept">
      <Meta name="internalId" value="10623"/>
      <Meta name="howApplies" value="other"/>
      <Rubric id="d1e91344" kind="definition">
         <Label xml:lang="en-US">
            <p>A compressed data format that also uses the deflate algorithm. Specified as RFC 1950 [http://www.ietf.org/rfc/rfc1952.txt]</p>
         </Label>
      </Rubric>
      <Rubric id="d1e91359" kind="short">
         <Label xml:lang="en-US">zlib</Label>
