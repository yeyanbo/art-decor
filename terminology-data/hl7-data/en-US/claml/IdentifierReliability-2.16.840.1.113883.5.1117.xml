      <Rubric id="d1e141111" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>The identifier was issued by the system responsible for constructing the instance.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e141127" kind="short">
         <Label xml:lang="en-US">Issued by System</Label>
      </Rubric>
      <Rubric id="d1e141127" kind="preferred">
