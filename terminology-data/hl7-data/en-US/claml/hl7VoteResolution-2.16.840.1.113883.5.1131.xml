         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Voter has formally withdrawn their vote or comment on the basis of agreed changes or proposed future changes.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139243" kind="short">
         <Label xml:lang="en-US">withdrawn</Label>
      </Rubric>
      <Rubric id="d1e139243" kind="preferred">
         <Label xml:lang="en-US">withdrawn</Label>
      </Rubric>
