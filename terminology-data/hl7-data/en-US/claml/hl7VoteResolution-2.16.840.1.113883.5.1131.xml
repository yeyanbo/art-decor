      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e139190" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Vote has not yet gone through resolution.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139211" kind="short">
         <Label xml:lang="en-US">unresolved</Label>
      </Rubric>
      <Rubric id="d1e139211" kind="preferred">
         <Label xml:lang="en-US">unresolved</Label>
      </Rubric>
   </Class>
