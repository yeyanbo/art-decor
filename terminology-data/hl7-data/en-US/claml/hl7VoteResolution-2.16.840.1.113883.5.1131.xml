      <Rubric id="d1e138901" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>The recommended change has been deferred to consideration for a future release.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e138922" kind="short">
         <Label xml:lang="en-US">affirmative-deferred</Label>
      </Rubric>
      <Rubric id="d1e138922" kind="preferred">
         <Label xml:lang="en-US">affirmative-deferred</Label>
      </Rubric>
   </Class>
   <Class code="affi" kind="concept">
      <Meta name="internalId" value="23134"/>
      <SuperClass code="affirmativeResolution"/>
      <Rubric id="d1e138933" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>The recommended change has been incorporated or identified issue has been answered.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e138954" kind="short">
         <Label xml:lang="en-US">affirmative-incorporated</Label>
      </Rubric>
      <Rubric id="d1e138954" kind="preferred">
         <Label xml:lang="en-US">affirmative-incorporated</Label>
      </Rubric>
   </Class>
   <Class code="affr" kind="concept">
      <Meta name="internalId" value="23135"/>
      <SuperClass code="affirmativeResolution"/>
      <Rubric id="d1e138965" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>The recommended change has been refused and is not expected to be incorporated.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e138986" kind="short">
         <Label xml:lang="en-US">affirmative-rejected</Label>
      </Rubric>
      <Rubric id="d1e138986" kind="preferred">
         <Label xml:lang="en-US">affirmative-rejected</Label>
      </Rubric>
   </Class>
   <Class code="nonsubp" kind="concept">
      <Meta name="internalId" value="23139"/>
      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e138997" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Responsible group has recommended that the negative vote be considered non-substantive. (Issue raised does not provide sufficiently convincing reason to make changes to the item under ballot, or otherwise impede its adoption.)</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139018" kind="short">
         <Label xml:lang="en-US">non-substantive proposed</Label>
      </Rubric>
      <Rubric id="d1e139018" kind="preferred">
         <Label xml:lang="en-US">non-substantive proposed</Label>
      </Rubric>
   </Class>
   <Class code="nonsubv" kind="concept">
      <Meta name="internalId" value="23140"/>
      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e139029" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Ballot group has voted and declared the negative vote non-substantive.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139050" kind="short">
         <Label xml:lang="en-US">non-substantive voted</Label>
      </Rubric>
      <Rubric id="d1e139050" kind="preferred">
         <Label xml:lang="en-US">non-substantive voted</Label>
      </Rubric>
   </Class>
   <Class code="notrelp" kind="concept">
      <Meta name="internalId" value="23141"/>
      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e139062" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Responsible group has recommended that the negative vote be considered not-related. (Issue raised is not related to the current scope of the item under ballot, or does not prevent the item under ballot for being used for its defined intent. Recommended changes may be considered as part of future versions.) (Perhaps after further reading or explanation).</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139083" kind="short">
         <Label xml:lang="en-US">not related proposed</Label>
      </Rubric>
      <Rubric id="d1e139083" kind="preferred">
         <Label xml:lang="en-US">not related proposed</Label>
      </Rubric>
   </Class>
   <Class code="notrelv" kind="concept">
      <Meta name="internalId" value="23142"/>
      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e139094" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Ballot group has voted and declared the negative vote non-related.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139115" kind="short">
         <Label xml:lang="en-US">not related voted</Label>
      </Rubric>
      <Rubric id="d1e139115" kind="preferred">
         <Label xml:lang="en-US">not related voted</Label>
      </Rubric>
   </Class>
   <Class code="prevcons" kind="concept">
      <Meta name="internalId" value="23143"/>
      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e139126" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Committee identifies that the same issue has been raised as part of a previous ballot on the same element version and was found by the ballot group to be non-substantive or not related.)</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139147" kind="short">
         <Label xml:lang="en-US">previously considered</Label>
      </Rubric>
      <Rubric id="d1e139147" kind="preferred">
         <Label xml:lang="en-US">previously considered</Label>
      </Rubric>
   </Class>
   <Class code="retract" kind="concept">
      <Meta name="internalId" value="23144"/>
      <SuperClass code="negativeResolution"/>
      <Rubric id="d1e139158" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>Voter has formally withdrawn their vote or comment as having been in error. (Perhaps after further reading or explanation).</p>
         </Label>
      </Rubric>
      <Rubric id="d1e139179" kind="short">
         <Label xml:lang="en-US">retracted</Label>
      </Rubric>
      <Rubric id="d1e139179" kind="preferred">
         <Label xml:lang="en-US">retracted</Label>
      </Rubric>
   </Class>
