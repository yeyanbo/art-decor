      <Rubric id="d1e138874" kind="definition">
         <Label xml:lang="en-US">
            <p>
               <b>Description:</b>An abstract concept grouping resolutions that can be applied to negative ballot comments.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e138890" kind="short">
         <Label xml:lang="en-US">negative resolution</Label>
