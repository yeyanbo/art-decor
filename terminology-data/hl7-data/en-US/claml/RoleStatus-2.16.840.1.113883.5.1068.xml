   <Class code="normal" kind="concept">
      <Meta name="internalId" value="16000"/>
      <SubClass code="active"/>
      <SubClass code="cancelled"/>
      <SubClass code="pending"/>
      <SubClass code="suspended"/>
      <SubClass code="terminated"/>
      <Rubric id="d1e217822" kind="definition">
         <Label xml:lang="en-US">
            <p>The 'typical' state. Excludes "nullified" which represents the termination state of a Role instance that was created in error.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e217835" kind="short">
         <Label xml:lang="en-US">normal</Label>
      </Rubric>
      <Rubric id="d1e217835" kind="preferred">
         <Label xml:lang="en-US">normal</Label>
      </Rubric>
   </Class>
   <Class code="nullified" kind="concept">
      <Meta name="internalId" value="16004"/>
      <Rubric id="d1e217847" kind="definition">
         <Label xml:lang="en-US">
            <p>The state representing the termination of a Role instance that was created in error.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e217860" kind="short">
         <Label xml:lang="en-US">nullified</Label>
      </Rubric>
      <Rubric id="d1e217860" kind="preferred">
         <Label xml:lang="en-US">nullified</Label>
      </Rubric>
   </Class>
   <Class code="active" kind="concept">
      <Meta name="internalId" value="16001"/>
      <SuperClass code="normal"/>
      <Rubric id="d1e217871" kind="definition">
         <Label xml:lang="en-US">
            <p>The state representing the fact that the Entity is currently active in the Role.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e217889" kind="short">
         <Label xml:lang="en-US">active</Label>
      </Rubric>
      <Rubric id="d1e217889" kind="preferred">
         <Label xml:lang="en-US">active</Label>
      </Rubric>
   </Class>
   <Class code="cancelled" kind="concept">
      <Meta name="internalId" value="21306"/>
      <SuperClass code="normal"/>
      <Rubric id="d1e217900" kind="definition">
         <Label xml:lang="en-US">
            <p>The terminal state resulting from cancellation of the role prior to activation.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e217918" kind="short">
         <Label xml:lang="en-US">cancelled</Label>
      </Rubric>
      <Rubric id="d1e217918" kind="preferred">
         <Label xml:lang="en-US">cancelled</Label>
      </Rubric>
   </Class>
   <Class code="pending" kind="concept">
      <Meta name="internalId" value="21305"/>
      <SuperClass code="normal"/>
      <Rubric id="d1e217929" kind="definition">
         <Label xml:lang="en-US">
            <p>The state representing that fact that the role has not yet become active.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e217947" kind="short">
         <Label xml:lang="en-US">pending</Label>
      </Rubric>
      <Rubric id="d1e217947" kind="preferred">
         <Label xml:lang="en-US">pending</Label>
      </Rubric>
   </Class>
   <Class code="suspended" kind="concept">
      <Meta name="internalId" value="16002"/>
      <SuperClass code="normal"/>
      <Rubric id="d1e217958" kind="definition">
         <Label xml:lang="en-US">
            <p>The state that represents a suspension of the Entity playing the Role. This state is arrived at from the "active" state.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e217976" kind="short">
         <Label xml:lang="en-US">suspended</Label>
      </Rubric>
      <Rubric id="d1e217976" kind="preferred">
         <Label xml:lang="en-US">suspended</Label>
      </Rubric>
   </Class>
   <Class code="terminated" kind="concept">
      <Meta name="internalId" value="16003"/>
      <SuperClass code="normal"/>
      <Rubric id="d1e217987" kind="definition">
         <Label xml:lang="en-US">
            <p>The state representing the successful termination of the Role.</p>
         </Label>
      </Rubric>
      <Rubric id="d1e218005" kind="short">
         <Label xml:lang="en-US">terminated</Label>
      </Rubric>
      <Rubric id="d1e218005" kind="preferred">
         <Label xml:lang="en-US">terminated</Label>
      </Rubric>
   </Class>
